package app;

public class Turma {
	String codigo, diaHora, semestre, qtdVagas;
	
	Professor professor;
	
	public Turma(String codigo, String professor, String disciplina, String diaHora, String semestre, String vagas,
			String alunos) {
		this.codigo = codigo;
		this.diaHora = diaHora;
		this.qtdVagas = qtdVagas;
		this.semestre = semestre;
	}

}
